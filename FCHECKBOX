import java.awt.*;
public class Check{
public static void main(String args[]){
Frame f=new Frame("checkbox");
Checkbox c1=new Checkbox("c");
c1.setBounds(50,100,100,30);
Checkbox c2=new Checkbox("java",true);
c2.setBounds(50,150,100,30);
Checkbox c3=new Checkbox("c++");
c3.setBounds(50,200,100,30);
f.add(c1);
f.add(c2);
f.add(c3);
f.setSize(400,400);
f.setVisible(true);
f.setLayout(null);
}
}
