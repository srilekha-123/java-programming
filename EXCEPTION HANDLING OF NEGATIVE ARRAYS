import java.util.*;
public class Exampleexcep{
  public static void main(String[] args){
      int size;
      Scanner sc=new Scanner(System.in);
     try{
        size=sc.nextInt();
        int[] a=new int[size];
        System.out.println("length of array is:"+size);
     }catch(Exception e){
        System.out.println("java.lang.NegativeArraySizeException");
     }
  }
}
