import java.util.Scanner;
class StringMethods{
      Scanner sc=new Scanner(System.in);
      String s1=sc.next();
      String s2=sc.next();
      void stringMethods()
      {
         char ch=s1.charAt(2);
         System.out.println("char at index given is:"+ch);
         System.out.println("length of string is:"+s1.length());
         System.out.println("substring of s1 is:"+s1.substring(2,5));
         System.out.println("enter strin to concatenate:");
         String s3=sc.next();
         s1=s1.concat(s3);
         System.out.println(s1);
         String s4=s1.toUpperCase();
         System.out.println("uppercase string is:"+s4);
         String s5=s1.toLowerCase();
         System.out.println("lowercase string is:"+s5);
         System.out.println("checking if string is equal or not:"+s1.equals(s2));
         System.out.println("strings are equal or not:"+s1==s2);
      }
}
class StringPrint{
   public static void main(String args[]){
    StringMethods obj=new StringMethods();
    obj.stringMethods();
  }
}
   
